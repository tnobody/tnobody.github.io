  },
  "dependencies": {
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@tailwindcss/typography": "^0.2.0",
    "add": "^2.0.6",
    "clean-css": "^4.2.3",
    "eleventy-plugin-reading-time": "^0.0.1",
    "markdown-spellcheck": "^1.3.1",
    "node-fetch": "^2.6.0",
    "reveal.js": "^4.1.0",
    "rollup": "^2.38.4",
    "yarn": "^1.22.4"
  }
