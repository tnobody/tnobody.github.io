  "scripts": {
    "preeleventy": "mkdir -p _site/reveal-css && cp -R node_modules/reveal.js/dist/ _site/reveal-css",
    "eleventy": "eleventy --input=content/",
    "esbuild": "esbuild js/slides.js --bundle --outfile=_site/js/slides.js",
    "dev": "yarn eleventy --serve --watch",
